program ConsultaCEP;

uses
  Vcl.Forms,
  vPrincipal in 'view\vPrincipal.pas' {Form1},
  dtPrincipal in 'data\dtPrincipal.pas' {dmPrincipal: TDataModule},
  vCadastroEndereco in 'view\vCadastroEndereco.pas' {frmCadastroEndereco},
  files.interfaces in 'interface\files.interfaces.pas',
  endereco.model in 'model\endereco.model.pas',
  endereco.controller in 'controller\endereco.controller.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TdmPrincipal, dmPrincipal);
  Application.CreateForm(TfrmCadastroEndereco, frmCadastroEndereco);
  Application.Run;
end.
